public class JavaBasics
{
   public static void main(String[] args)
   {
      int matrix[][] = {{1,2,3},
                         {4,5,6},
                         {7,8,9}};
       int sum=0;
       for (int i = 0; i < matrix[0].length; i++) {
         sum+=matrix[i][0];
         
       }    
       System.out.print(sum);              
   }
}
