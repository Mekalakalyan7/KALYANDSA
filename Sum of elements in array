
//Using loop
import java.util.stream.IntStream;

public class JavaBasics
{
   public static void main(String[] args) {
      int arr[] = {5,4,1,3,2};
     
      int sum = IntStream.of(arr).sum();
      System.out.println(sum);
   }
}

//using library function
import java.util.stream.IntStream;

public class JavaBasics
{
   public static void main(String[] args) {
      int arr[] = {5,4,1,3,2};
     
      int sum = IntStream.of(arr).sum();
      System.out.println(sum);
   }
}
