public class JavaBasics
{

    public static int getLowest(int arr[])
    {
        int lowestVal = arr[0];
        for(int i=0;i<arr.length;i++)
        {
            if(arr[i]<lowestVal)
            {
                lowestVal=arr[i];
            }
        }
            return lowestVal;
    }
    public static void main(String[] args) {
        int arr[] = {5,4,11,3,12};
       System.out.println(getLowest(arr));
       
    }
}
